import math
x = input ("Please input a whole number betwqeen 1 and 20: ")
x = int(x)
if x < 21:
  if x > 0:
    y = input ("Please input another whole number betwqeen 1 and 20: ")
    y = int(y)
    if y < 21:
      if y > 0:
        rem = x % y
        if rem == 0:
          print("Yes!", y, "is a factor of", x)
          
        else:
          print(y, "is not a factor of ",x ,".")
          
      else:
        print(y, "is outside of range. Cannot continue")
        
    else:
      print(y, "is outside of range. Cannot continue")
      
  else:
    print(x, "is outside of range. Cannot continue")
    
else:
  print(x, "is outside of range. Cannot continue")
